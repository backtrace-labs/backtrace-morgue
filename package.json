{
  "name": "backtrace-morgue",
  "version": "1.24.11",
  "description": "command line interface to the Backtrace object store",
  "main": "./dist/lib/coroner.js",
  "engines": {
    "node": ">=14.17.0"
  },
  "engineStrict": true,
  "keywords": [
    "backtrace",
    "crashes",
    "post-mortem",
    "debugging"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backtrace-labs/backtrace-morgue.git"
  },
  "author": "Backtrace <team@backtrace.io>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/backtrace-labs/backtrace-morgue/issues"
  },
  "homepage": "https://github.com/backtrace-labs/backtrace-morgue#readme",
  "dependencies": {
    "@backtrace/node": "^0.3.1",
    "@cypress/request": "^3.0.8",
    "assign-deep": "^1.0.1",
    "axios": "^1.10.0",
    "btoa": "^1.1.2",
    "chalk": "^4.1.2",
    "chrono-node": "^2.2.4",
    "clone": "^2.1.0",
    "csv-writer": "^1.6.0",
    "extsprintf": "^1.3.0",
    "ip6addr": "^0.2.5",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "printf": "^0.6.1",
    "prompt": "^1.3.0",
    "streamsink": "^1.2.0",
    "sync-request": "^5.0.0",
    "table": "^4.0.1",
    "time-ago": "^0.2.1",
    "url-join": "^4.0.1",
    "winston": "2.4.5"
  },
  "preferGlobal": true,
  "bin": {
    "morgue": "./dist/bin/morgue.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-env": "^7.28.0",
    "@types/btoa": "^1.2.5",
    "@types/clone": "^2.1.4",
    "@types/extsprintf": "^1.4.4",
    "@types/minimist": "^1.2.5",
    "@types/mkdirp": "^0.5.1",
    "@types/node": "^14.18.63",
    "@types/printf": "^0.2.1",
    "@types/prompt": "^1.1.9",
    "@types/table": "^6.0.0",
    "@types/url-join": "^4.0.3",
    "del": "^3.0.0",
    "eslint": "^7.12.1",
    "gulp": "^5.0.1",
    "gulp-babel": "^8.0.0",
    "typescript": "^5.8.3",
    "workaround-broken-npm-prepack-behavior": "https://github.com/cspotcode/workaround-broken-npm-prepack-behavior#59e32763ce19d087062b6ce287a21ee00d9a7187"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "tsc -b --clean",
    "compile": "tsc && npm run copy-assets",
    "copy-assets": "cp -r assets dist/",
    "lint": "eslint",
    "prepare": "workaround-broken-npm-prepack-behavior prepack",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch"
  }
}
